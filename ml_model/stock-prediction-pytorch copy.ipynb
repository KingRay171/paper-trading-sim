   "source": [
    "def make_prediction(x_test, model, size):\n",
    "    X=x_test[-1:,...,...]\n",
    "    with torch.no_grad():\n",
    "        predictions = []\n",
    "        model.eval()\n",
    "        for _ in range(size):\n",
    "            yhat = model(X)\n",
    "            X=torch.roll(X, shifts=-1, dims=1)\n",
    "            X[0,-1,0]=yhat\n",
    "            predictions.append(yhat.item())\n",
    "        return torch.tensor(predictions).reshape(-1, 1)\n",
    "                "
   ]
